    "lodash": "^4.14.1",
    "sanitize-html": "^1.13.0",
    "url-composer": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2040": "^1.1.1",
    "browser-sync": "^2.14.0",
    "browser-sync-webpack-plugin": "^1.0.1",
    "connect-history-api-fallback": "^1.2.0",
    "connect-logger": "0.0.1",
